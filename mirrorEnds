public String mirrorEnds(String string) {
  StringBuilder sb = new StringBuilder();
  
  for (int i = 0; i < string.length(); i++){
    if (string.charAt(i) == string.charAt(string.length()-1-i)){
      sb.append(string.charAt(i));
    }else{
      break;
    }
    
    
    
  }
  
  return new String(sb);
}
